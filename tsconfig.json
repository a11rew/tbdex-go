{
	"compilerOptions": {
		"target": "es2021",
		"module": "es2022",
		"moduleResolution": "node",
		"jsx": "react-jsx",
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"]
		}
	},
	"include": ["src/**/*", "worker-configuration.d.ts"]
}
